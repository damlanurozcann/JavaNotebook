package labaratuvar14;

public class GradedActivity {
	
	private double score;

	public double getScore() {
		return score;
	}

	public void setScore(double score) {
		this.score = score;
	}
	
	public char getGrade() {
		
		char letterGrade; //harfli başarı notu
		
		if (score>=90) {
			letterGrade='A';
		} else if (score>=80) {
			letterGrade='B';
			
		}
		
		
		
		
		return letterGrade;
	}
}
